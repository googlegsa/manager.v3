<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>  <bean id="SessionManager"
    class="com.google.enterprise.security.manager.LocalSessionManager">
  </bean>

  <bean id="ArtifactResolver"
    class="com.google.enterprise.saml.server.ArtifactResolverImpl">
  </bean>

  <bean id="AuthzResponder"
    class="com.google.enterprise.saml.server.AuthzResponderImpl">
  </bean>

	<bean id="BackEnd"		class="com.google.enterprise.saml.server.BackEndImpl">    <property name="sessionManager" ref="SessionManager"/>
    <property name="artifactResolver" ref="ArtifactResolver"/>
    <property name="authzResponder" ref="AuthzResponder"/>
	</bean>
</beans>
